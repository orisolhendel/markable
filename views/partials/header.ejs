<style>
button {
            font-family: 'Quattrocento';
            font-size: 18px;
            color: #ffffff;
            border: none;
            border-radius: 12px;
            background: #ff0083;
            cursor: pointer;
            text-align: center;
}

button:hover {
    background-color:#db0272;
}

button.selfieButton {
    width: 140px;
    height: 38px;;
    background-color: #ffffff;
    color:#ff0083;
    border: 1px solid #ff0083;
}

button.selfieButton:hover {
    color:#db0272;
    border: 1px solid #db0272;
}

a { 
    font-family: 'Quattrocento';
    font-size: 24px;
    color: #ff0083;
    text-decoration: none;
    cursor: pointer;
}

a:hover {
    text-decoration: underline;
}

.captionText {
    font-family: 'Lora'; 
    font-size:18px;
    padding: 10px;
    color:#777777;
    display: inline;
    padding-right: 20px;
}

.abs {
    position: absolute;
}

.fxd {
    position: fixed;
}

.hand {
    cursor: pointer;
}

.flex {
    display: flex;
}

@media only screen and (max-width: 1000px) {
  div.hideForMobile {
    visibility: hidden;
  }
}

@media only screen and (min-width: 1000px) {
  div.onlyForMobile {
    display: none;
  }
}
</style>

<script src="./scripts/utils.js"></script>
<script src="./scripts/mark_hamburgers.js"></script>

<!-- top caption -->
<div class="abs" style="left: 80px; top: 10px; z-index: 200;">
    <img src="../img/logo.png" class="hand" width="300px" height="auto" onclick="location.href = '/'">
</div>

<div class="abs" style="box-shadow: rgba(0, 0, 0, 0.15) 0px 0px 25px; width:100%; height: 58px; right: 0px; top:0px; background-color: #ffffff; margin: 0%;">
    <div class="abs" style="right: 0px; top:18px;">
        <div class="hideForMobile captionText" onclick="location.href = 'javascript:void(0)'" >
            <a class="captionText">About</a>
        </div>
        <div class="hideForMobile captionText" onclick="location.href = 'javascript:void(0)'">
            <a class="captionText">Technology</a>
        </div>
        <div class="hideForMobile captionText" onclick="location.href = 'javascript:void(0)'">
            <a class="captionText">FAQ</a>
        </div>
        <div class="hideForMobile captionText" onclick="location.href = 'javascript:void(0)'">
            <a class="captionText">Treatment Options</a>
        </div>
        <div class="hideForMobile captionText" onclick="showResults('test')">
            <a class="captionText">Articles</a>
        </div>
        <div class="abs onlyForMobile" style="top:-8px; right:-10px;">
            <%- include('./mark_hamburger', {hamb: "main", menu:["About", "Technology", "FAQ", "Treatment Options", "Articles"]}); %>
